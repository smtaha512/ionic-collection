{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "module": "esnext",
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "es2015",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
      "segments": [
        "dist/segments"
      ],
      "segments/*": [
        "dist/segments/*"
      ],
      "@angular/*": [
        "./node_modules/@angular/*"
      ],
      "@ionic/angular": [
        "./node_modules/@ionic/angular"
      ],
      "accordion": [
        "dist/accordion"
      ],
      "accordion/*": [
        "dist/accordion/*"
      ]
    }
  },
  "angularCompilerOptions": {
    "fullTemplateTypeCheck": true,
    "strictInjectionParameters": true
  }
}